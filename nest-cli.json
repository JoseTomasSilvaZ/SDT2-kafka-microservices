{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/sdt2-kafka/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/sdt2-kafka/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/sdt2-kafka",
  "projects": {
    "sdt2-kafka": {
      "type": "application",
      "root": "apps/sdt2-kafka",
      "entryFile": "main",
      "sourceRoot": "apps/sdt2-kafka/src",
      "compilerOptions": {
        "tsConfigPath": "apps/sdt2-kafka/tsconfig.app.json"
      }
    },
    "orders-service": {
      "type": "application",
      "root": "apps/orders-service",
      "entryFile": "main",
      "sourceRoot": "apps/orders-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/orders-service/tsconfig.app.json"
      }
    },
    "notifications-service/": {
      "type": "application",
      "root": "apps/notifications-service",
      "entryFile": "main",
      "sourceRoot": "apps/notifications-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/notifications-service/tsconfig.app.json"
      }
    },
    "processing-service/": {
      "type": "application",
      "root": "apps/processing-service",
      "entryFile": "main",
      "sourceRoot": "apps/processing-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/processing-service/tsconfig.app.json"
      }
    },
    "orders": {
      "type": "application",
      "root": "apps/orders",
      "entryFile": "main",
      "sourceRoot": "apps/orders/src",
      "compilerOptions": {
        "tsConfigPath": "apps/orders/tsconfig.app.json"
      }
    },
    "processing": {
      "type": "application",
      "root": "apps/processing",
      "entryFile": "main",
      "sourceRoot": "apps/processing/src",
      "compilerOptions": {
        "tsConfigPath": "apps/processing/tsconfig.app.json"
      }
    },
    "notifications": {
      "type": "application",
      "root": "apps/notifications",
      "entryFile": "main",
      "sourceRoot": "apps/notifications/src",
      "compilerOptions": {
        "tsConfigPath": "apps/notifications/tsconfig.app.json"
      }
    },
    "prisma": {
      "type": "library",
      "root": "libs/prisma",
      "entryFile": "index",
      "sourceRoot": "libs/prisma/src",
      "compilerOptions": {
        "tsConfigPath": "libs/prisma/tsconfig.lib.json"
      }
    }
  }
}